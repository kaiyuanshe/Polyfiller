{
	"name": "@wessberg/polyfiller",
	"version": "0.0.116",
	"description": "Never worry about polyfills again.",
	"files": [
		"dist/**/*.*",
		"polyfill-lib/**/*.*"
	],
	"scripts": {
		"start": "node ./dist/index.js",
		"generate:scaffold": "scaffold all --yes",
		"generate:changelog": "standard-changelog --first-release",
		"generate:all": "npm run generate:scaffold && npm run generate:changelog",
		"clean": "rimraf dist",
		"lint": "tsc --noEmit && eslint \"src/**/*.ts\" --color",
		"prettier": "prettier --write '{src,test,documentation}/**/*.{js,ts,json,html,xml,css,md}'",
		"test": "NODE_ENV=TEST ava",
		"prebuild": "npm run clean",
		"build": "npm run rollup",
		"watch": "npm run rollup -- --watch",
		"rollup": "rollup -c rollup.config.js",
		"scripts:markdown-format-feature-names": "ts-node --project scripts/tsconfig.json scripts/markdown-format-feature-names.ts",
		"preversion": "NODE_ENV=production npm run build",
		"version": "npm run generate:all && git add .",
		"release": "np --no-cleanup --no-yarn",
		"update": "npx npm-check-updates -u -x np --dep dev,prod && npm update && npm install"
	},
	"keywords": [
		"polyfills",
		"shim",
		"web",
		"webservice",
		"typescript",
		"polyfill-app",
		"polyfills-as-a-service"
	],
	"devDependencies": {
		"@types/astring": "1.3.0",
		"@types/mime": "2.0.3",
		"@types/node": "14.14.9",
		"@types/semver": "7.3.4",
		"@typescript-eslint/eslint-plugin": "4.8.1",
		"@typescript-eslint/parser": "4.8.1",
		"@wessberg/di-compiler": "2.1.1",
		"@wessberg/rollup-plugin-ts": "1.3.8",
		"@wessberg/scaffold": "1.0.32",
		"@wessberg/ts-config": "1.0.20",
		"ava": "3.13.0",
		"eslint": "7.14.0",
		"eslint-config-prettier": "6.15.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsdoc": "30.7.8",
		"husky": "4.3.0",
		"rollup": "2.33.3",
		"node-ssh": "^11.1.1",
		"np": "^6.5.0",
		"prettier": "2.2.0",
		"pretty-quick": "3.1.0",
		"rimraf": "3.0.2",
		"standard-changelog": "2.0.27",
		"ts-node": "9.0.0",
		"typescript": "4.1.2",
		"useragent-generator": "1.1.1-amkt-22079-finish.0"
	},
	"dependencies": {
		"@babel/core": "7.12.7",
		"@babel/plugin-transform-block-scoping": "7.12.1",
		"@webcomponents/custom-elements": "1.4.3",
		"@webcomponents/shadycss": "1.10.2",
		"@webcomponents/shadydom": "1.8.0",
		"@webcomponents/template": "1.4.4",
		"@wessberg/browserslist-generator": "1.0.42",
		"@wessberg/di": "2.0.3",
		"browserslist": "4.14.7",
		"@wessberg/fileloader": "1.1.12",
		"@wessberg/filesaver": "1.0.11",
		"@wessberg/pointer-events": "1.0.9",
		"@wessberg/stringutil": "1.0.19",
		"esbuild": "0.8.12",
		"@swc/core": "1.2.39",
		"Base64": "1.1.0",
		"astring": "1.4.3",
		"blob-polyfill": "4.0.20200601",
		"chalk": "4.1.0",
		"console-polyfill": "0.3.0",
		"construct-style-sheets-polyfill": "2.4.3",
		"core-js": "3.7.0",
		"events-polyfill": "2.1.2",
		"find-up": "5.0.0",
		"http-status-codes": "2.1.4",
		"intersection-observer": "0.11.0",
		"@formatjs/intl-datetimeformat": "3.0.0",
		"@formatjs/intl-displaynames": "4.0.0",
		"@formatjs/intl-getcanonicallocales": "1.5.1",
		"@formatjs/intl-listformat": "5.0.0",
		"@formatjs/intl-locale": "2.4.7",
		"@formatjs/intl-numberformat": "6.0.0",
		"@formatjs/intl-pluralrules": "4.0.0",
		"@formatjs/intl-relativetimeformat": "8.0.0",
		"mime": "~2.4.6",
		"mutationobserver-shim": "0.3.7",
		"node.parentelement": "1.0.2",
		"@polyfiller/object-fit": "0.0.39",
		"perfnow": "0.2.0",
		"cjstoesm": "0.1.3",
		"polyfill-library": "3.97.0",
		"proxy-polyfill": "0.3.2",
		"regenerator-runtime": "0.13.7",
		"requestanimationframe": "0.0.23",
		"requestidlecallback": "0.3.0",
		"scroll-behavior-polyfill": "2.0.13",
		"semver": "7.3.2",
		"setimmediate": "1.0.5",
		"systemjs": "6.7.1",
		"temp-dir": "2.0.0",
		"toposort": "2.0.2",
		"tslib": "2.0.3",
		"url-polyfill": "1.1.12",
		"web-animations-js": "2.3.2",
		"whatwg-fetch": "3.5.0",
		"xhr-polyfill": "0.1.8",
		"focus-visible": "5.2.0",
		"zone.js": "0.11.3",
		"pm2": "4.5.0",
		"formdata-polyfill": "3.0.20"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"browser": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typings": "./dist/index.d.ts",
	"es2015": "./dist/index.js",
	"funding": {
		"type": "github",
		"url": "https://github.com/wessberg/polyfiller?sponsor=1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wessberg/polyfiller.git"
	},
	"bugs": {
		"url": "https://github.com/wessberg/polyfiller/issues"
	},
	"contributors": [
		{
			"name": "Frederik Wessberg",
			"email": "frederikwessberg@hotmail.com",
			"url": "https://github.com/wessberg",
			"imageUrl": "https://avatars2.githubusercontent.com/u/20454213?s=460&v=4",
			"github": "wessberg",
			"role": "Lead Developer",
			"twitter": "FredWessberg"
		}
	],
	"engines": {
		"node": ">=10.16.0"
	},
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"ava": {
		"files": [
			"test/**/**.test.ts"
		],
		"verbose": true,
		"timeout": "300s",
		"extensions": [
			"ts"
		],
		"environmentVariables": {
			"NODE_OPTIONS": "--max_old_space_size=4096",
			"TESTING": "true"
		},
		"require": [
			"@wessberg/di-compiler/register"
		]
	}
}
