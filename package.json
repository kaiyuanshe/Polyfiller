{
	"name": "@wessberg/polyfiller",
	"version": "0.0.93",
	"description": "Never worry about polyfills again.",
	"files": [
		"dist/**/*.*",
		"polyfill-lib/**/*.*"
	],
	"scripts": {
		"serve": "node ./dist/index.js",
		"serve:production": "NODE_ENV=production npm run serve",
		"generate:scaffold": "scaffold all --yes",
		"generate:changelog": "standard-changelog --first-release",
		"generate:all": "npm run generate:scaffold && npm run generate:changelog",
		"clean": "rimraf dist",
		"lint": "tsc --noEmit && eslint \"src/**/*.ts\" --color",
		"prettier": "prettier --write '{src,test,documentation}/**/*.{js,ts,json,html,xml,css,md}'",
		"pretest": "rimraf compiled && npm run rollup:test",
		"test": "NODE_ENV=TEST ava",
		"posttest": "rimraf compiled",
		"prebuild": "npm run clean",
		"build": "npm run rollup",
		"watch": "npm run rollup -- --watch",
		"rollup": "rollup -c rollup.config.js",
		"rollup:test": "rollup -c rollup.test.config.js",
		"scripts:markdown-format-feature-names": "ts-node --project scripts/tsconfig.json scripts/markdown-format-feature-names.ts",
		"preversion": "npm run lint && NODE_ENV=production npm run build",
		"version": "npm run generate:all && git add .",
		"postpublish": "npm run deploy",
		"release": "np --no-cleanup --no-yarn",
		"deploy": "nanobox build && nanobox deploy"
	},
	"keywords": [
		"polyfills",
		"shim",
		"web",
		"webservice",
		"typescript",
		"polyfill-app",
		"polyfills-as-a-service"
	],
	"devDependencies": {
		"@wessberg/di-compiler": "^2.0.5",
		"@wessberg/rollup-plugin-ts": "^1.2.18",
		"@wessberg/scaffold": "^1.0.25",
		"@wessberg/ts-config": "^1.0.8",
		"@typescript-eslint/eslint-plugin": "^2.21.0",
		"@typescript-eslint/parser": "^2.21.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jsdoc": "^21.0.0",
		"husky": "^4.2.3",
		"np": "5.2.1",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"rimraf": "^3.0.2",
		"standard-changelog": "^2.0.21",
		"ts-node": "^8.6.2",
		"typescript": "^3.8.3",
		"useragent-generator": "^1.1.1-amkt-22079-finish.0"
	},
	"dependencies": {
		"@babel/core": "^7.8.6",
		"@babel/plugin-transform-block-scoping": "^7.8.3",
		"@types/astring": "^1.3.0",
		"@types/iltorb": "^2.3.0",
		"@types/memory-fs": "^0.3.2",
		"@types/mime": "^2.0.1",
		"@types/node": "^12.7.8",
		"@types/semver": "^6.0.2",
		"@webcomponents/custom-elements": "^1.3.0",
		"@webcomponents/shadycss": "^1.9.2",
		"@webcomponents/shadydom": "^1.6.1",
		"@webcomponents/template": "^1.4.1",
		"@wessberg/browserslist-generator": "1.0.27",
		"@wessberg/di": "^2.0.3",
		"@wessberg/fileloader": "^1.1.11",
		"@wessberg/filesaver": "^1.0.10",
		"@wessberg/pointer-events": "^1.0.9",
		"@wessberg/stringutil": "^1.0.19",
		"Base64": "^1.0.2",
		"astring": "^1.4.2",
		"ava": "^3.4.0",
		"blob-polyfill": "^4.0.20190430",
		"chalk": "^2.4.2",
		"console-polyfill": "^0.3.0",
		"construct-style-sheets-polyfill": "^2.2.10",
		"core-js": "^3.2.1",
		"core-js-builder": "^3.2.1",
		"devcert": "^1.0.2",
		"events-polyfill": "^2.1.2",
		"file-type": "^12.3.0",
		"find-up": "^4.1.0",
		"http-status-codes": "^1.3.2",
		"iltorb": "^2.4.3",
		"intersection-observer": "^0.7.0",
		"intl": "^1.2.5",
		"intl-list-format": "^1.0.3",
		"intl-relative-time-format": "^1.0.7",
		"memory-fs": "^0.4.1",
		"mime": "^2.4.4",
		"mutationobserver-shim": "^0.3.3",
		"node.parentelement": "^1.0.2",
		"nodemon": "^2.0.2",
		"object-fit-images": "^3.2.4",
		"perfnow": "^0.2.0",
		"polyfill-library": "^3.41.0",
		"proxy-polyfill": "^0.3.0",
		"regenerator-runtime": "^0.13.3",
		"requestanimationframe": "0.0.23",
		"requestidlecallback": "^0.3.0",
		"rollup": "^1.32.0",
		"@rollup/plugin-commonjs": "^11.0.2",
		"@rollup/plugin-multi-entry": "^3.0.0",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"scroll-behavior-polyfill": "^2.0.12",
		"semver": "^6.3.0",
		"setimmediate": "^1.0.5",
		"systemjs": "^6.1.2",
		"temp-dir": "^2.0.0",
		"toposort": "^2.0.2",
		"tslib": "^1.10.0",
		"url-polyfill": "^1.1.7",
		"web-animations-js": "^2.3.2",
		"whatwg-fetch": "^3.0.0",
		"xhr-polyfill": "^0.1.8",
		"zone.js": "^0.10.2"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"browser": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typings": "./dist/index.d.ts",
	"es2015": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/wessberg/polyfiller.git"
	},
	"bugs": {
		"url": "https://github.com/wessberg/polyfiller/issues"
	},
	"contributors": [
		{
			"name": "Frederik Wessberg",
			"email": "frederikwessberg@hotmail.com",
			"url": "https://github.com/wessberg",
			"imageUrl": "https://avatars2.githubusercontent.com/u/20454213?s=460&v=4",
			"github": "wessberg",
			"role": "Lead Developer",
			"twitter": "FredWessberg"
		}
	],
	"engines": {
		"node": ">=9.0.0"
	},
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"ava": {
		"files": [
			"compiled/*.test.*"
		],
		"verbose": true,
		"timeout": "40s",
		"extensions": [
			"js"
		],
		"environmentVariables": {
			"NODE_OPTIONS": "--max_old_space_size=4096"
		}
	}
}
