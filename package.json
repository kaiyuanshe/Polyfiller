{
	"name": "@wessberg/polyfiller",
	"version": "0.0.14",
	"description": "Never worry about polyfills again.",
	"scripts": {
		"changelog:generate": "conventional-changelog --outfile CHANGELOG.md --release-count 0",
		"readme:badges": "node node_modules/@wessberg/ts-config/readme/badge/helper/add-badges.js",
		"readme:refresh": "npm run changelog:generate && npm run readme:badges",
		"commit:readme": "npm run readme:refresh && git commit -am \"Bumped version\" --no-verify || true",
		"clean:dist": "rm -r -f dist",
		"clean:compiled": "rm -r -f compiled",
		"clean": "npm run clean:dist && npm run clean:compiled",
		"rollup": "rollup -c rollup.config.js",
		"rollup:watch": "rollup -c rollup.config.js --watch",
		"rollup:test": "rollup -c rollup.test.config.js",
		"pretest": "npm run clean:compiled && npm run rollup:test",
		"test": "NODE_ENV=TEST ava --fail-fast",
		"posttest": "npm run clean:compiled",
		"prebuild": "npm run clean",
		"build": "npm run rollup",
		"serve": "nodemon --delay 800ms ./dist/index.js",
		"serve:production": "NODE_ENV=production node ./dist/index.js",
		"bs": "npm run build && npm run serve",
		"bs:watch": "npm run watch & npm run serve",
		"watch": "npm run rollup:watch",
		"tslint": "tslint -c tslint.json -p tsconfig.json",
		"validate": "npm run tslint",
		"prepublishOnly": "NODE_ENV=production npm run validate && NODE_ENV=production npm run build",
		"precommit": "npm run tslint && exit 0",
		"prepush": "npm run validate && exit 0",
		"publish:major": "npm version major && npm run commit:readme && git push && NODE_ENV=production npm publish",
		"publish:minor": "npm version minor && npm run commit:readme && git push && NODE_ENV=production npm publish",
		"publish:patch": "npm version patch && npm run commit:readme && git push && NODE_ENV=production npm publish",
		"deploy": "NODE_ENV=production npm run build && nanobox build && nanobox deploy"
	},
	"keywords": [],
	"devDependencies": {
		"@wessberg/rollup-plugin-di": "^1.0.80",
		"@wessberg/ts-config": "^0.0.27",
		"conventional-changelog-cli": "^2.0.1",
		"husky": "latest",
		"rollup": "^0.59.4",
		"tslint": "^5.10.0",
		"typescript": "^2.9.1",
		"useragent-generator": "^1.0.3"
	},
	"dependencies": {
		"@babel/core": "^7.0.0-beta.49",
		"@babel/preset-env": "^7.0.0-beta.49",
		"@babel/preset-stage-3": "^7.0.0-beta.49",
		"@babel/preset-typescript": "^7.0.0-beta.49",
		"@types/file-type": "^5.2.1",
		"@types/iltorb": "^2.0.1",
		"@types/mime": "^2.0.0",
		"@types/node": "^10.3.1",
		"@types/semver": "^5.5.0",
		"@webcomponents/custom-elements": "^1.1.1",
		"@webcomponents/shadydom": "^1.0.14",
		"@webcomponents/template": "^1.2.4",
		"@wessberg/browserslist-generator": "0.0.30",
		"@wessberg/di": "^1.0.26",
		"@wessberg/fileloader": "^1.1.9",
		"@wessberg/filesaver": "^1.0.8",
		"@wessberg/marshaller": "^2.0.5",
		"@wessberg/rollup-plugin-ts": "0.0.18",
		"Base64": "^1.0.1",
		"ava": "^0.25.0",
		"babel-minify": "^0.4.3",
		"blob-polyfill": "^2.0.20171115",
		"chalk": "^2.4.1",
		"console-polyfill": "^0.3.0",
		"core-js": "^2.5.7",
		"core-js-builder": "^2.5.7",
		"devcert": "^1.0.0",
		"events-polyfill": "^2.0.5",
		"file-type": "^8.0.0",
		"http-status-codes": "^1.3.0",
		"iltorb": "^2.3.2",
		"intersection-observer": "^0.5.0",
		"intl": "^1.2.5",
		"mime": "^2.3.1",
		"mutationobserver-shim": "^0.3.2",
		"node.parentelement": "^1.0.2",
		"nodemon": "^1.17.5",
		"object-fit-images": "^3.2.3",
		"pepjs": "^0.4.3",
		"perfnow": "^0.2.0",
		"polyfill-service": "^3.25.1",
		"proxy-polyfill": "^0.3.0",
		"regenerator-runtime": "^0.11.1",
		"requestanimationframe": "0.0.23",
		"rollup-plugin-node-resolve": "^3.3.0",
		"semver": "^5.5.0",
		"systemjs": "^0.21.4",
		"temp-dir": "^1.0.0",
		"tslib": "^1.9.2",
		"url-polyfill": "^1.0.13",
		"web-animations-js": "^2.3.1",
		"whatwg-fetch": "^2.0.4",
		"xhr-polyfill": "^0.1.8",
		"zone.js": "^0.8.26"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"browser": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typings": "./dist/index.d.ts",
	"es2015": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/wessberg/polyfiller.git"
	},
	"bugs": {
		"url": "https://github.com/wessberg/polyfiller/issues"
	},
	"author": {
		"name": "Frederik Wessberg",
		"email": "frederikwessberg@hotmail.com",
		"url": "https://github.com/wessberg"
	},
	"engines": {
		"node": ">=9.0.0"
	},
	"license": "MIT"
}
