{
	"name": "@wessberg/polyfiller",
	"version": "0.0.79",
	"description": "Never worry about polyfills again.",
	"files": [
		"dist/**/*.*",
		"polyfill-lib/**/*.*"
	],
	"scripts": {
		"serve": "node ./dist/index.js",
		"serve:production": "NODE_ENV=production npm run serve",
		"generate:readme": "scaffold readme --yes",
		"generate:license": "scaffold license --yes",
		"generate:contributing": "scaffold contributing --yes",
		"generate:coc": "scaffold coc --yes",
		"generate:changelog": "standard-changelog --first-release",
		"generate:all": "npm run generate:license & npm run generate:contributing & npm run generate:coc & npm run generate:readme && npm run generate:changelog",
		"clean:dist": "rm -rf dist",
		"clean:compiled": "rm -rf compiled",
		"clean": "npm run clean:dist && npm run clean:compiled",
		"lint": "tsc --noEmit && tslint -c tslint.json --project tsconfig.json",
		"prettier": "prettier --write '{src,test,documentation}/**/*.{js,ts,json,html,xml,css,md}'",
		"rollup:test": "rollup -c rollup.test.config.js",
		"pretest": "npm run clean:compiled && npm run rollup:test",
		"test": "NODE_ENV=TEST ava",
		"posttest": "npm run clean:compiled",
		"prebuild": "npm run clean:dist",
		"build": "npm run rollup",
		"rollup": "rollup -c rollup.config.js",
		"scripts:markdown-format-feature-names": "ts-node --project scripts/tsconfig.json scripts/markdown-format-feature-names.ts",
		"preversion": "npm run lint && NODE_ENV=production npm run build",
		"version": "npm run generate:all && git add .",
		"postpublish": "npm run deploy",
		"release": "np --no-cleanup --no-yarn",
		"deploy": "nanobox build && nanobox deploy"
	},
	"keywords": [
		"polyfills",
		"shim",
		"web",
		"webservice",
		"typescript",
		"polyfill-app",
		"polyfills-as-a-service"
	],
	"devDependencies": {
		"@wessberg/di-compiler": "^2.0.2",
		"@wessberg/rollup-plugin-ts": "1.1.45",
		"@wessberg/scaffold": "^1.0.17",
		"@wessberg/ts-config": "^0.0.39",
		"husky": "^1.3.1",
		"np": "^4.0.2",
		"prettier": "^1.16.4",
		"pretty-quick": "^1.10.0",
		"standard-changelog": "^2.0.7",
		"ts-node": "^8.0.3",
		"tslint": "^5.15.0",
		"typescript": "^3.4.2",
		"useragent-generator": "^1.1.1-amkt-22079-finish.0"
	},
	"dependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-transform-block-scoping": "^7.4.0",
		"@types/astring": "^1.3.0",
		"@types/file-type": "^10.9.1",
		"@types/iltorb": "^2.3.0",
		"@types/memory-fs": "^0.3.2",
		"@types/mime": "^2.0.1",
		"@types/node": "^11.13.0",
		"@types/semver": "^6.0.0",
		"@webcomponents/custom-elements": "^1.2.2",
		"@webcomponents/shadycss": "^1.9.0",
		"@webcomponents/shadydom": "^1.5.0",
		"@webcomponents/template": "^1.4.0",
		"@wessberg/browserslist-generator": "1.0.15",
		"@wessberg/di": "^2.0.3",
		"@wessberg/fileloader": "^1.1.9",
		"@wessberg/filesaver": "^1.0.8",
		"@wessberg/pointer-events": "^1.0.9",
		"@wessberg/stringutil": "^1.0.18",
		"Base64": "^1.0.2",
		"astring": "^1.4.0",
		"ava": "^1.4.1",
		"blob-polyfill": "^3.0.20180112",
		"chalk": "^2.4.2",
		"console-polyfill": "^0.3.0",
		"construct-style-sheets-polyfill": "^1.0.3",
		"core-js": "^3.0.1",
		"core-js-builder": "^3.0.1",
		"devcert": "^1.0.0",
		"events-polyfill": "^2.1.0",
		"file-type": "^10.10.0",
		"find-up": "^3.0.0",
		"http-status-codes": "^1.3.2",
		"iltorb": "^2.4.2",
		"intersection-observer": "^0.5.1",
		"intl": "^1.2.5",
		"intl-list-format": "^1.0.2",
		"intl-relative-time-format": "^1.0.6",
		"memory-fs": "^0.4.1",
		"mime": "^2.4.1",
		"mutationobserver-shim": "^0.3.3",
		"node.parentelement": "^1.0.2",
		"nodemon": "^1.18.10",
		"object-fit-images": "^3.2.4",
		"perfnow": "^0.2.0",
		"polyfill-service": "^3.25.3",
		"proxy-polyfill": "^0.3.0",
		"regenerator-runtime": "^0.13.2",
		"requestanimationframe": "0.0.23",
		"requestidlecallback": "^0.3.0",
		"rollup": "^1.9.0",
		"rollup-plugin-commonjs": "^9.3.4",
		"rollup-plugin-multi-entry": "^2.1.0",
		"rollup-plugin-node-resolve": "^4.2.1",
		"scroll-behavior-polyfill": "^2.0.7",
		"semver": "^6.0.0",
		"setimmediate": "^1.0.5",
		"systemjs": "^3.1.0",
		"temp-dir": "^1.0.0",
		"toposort": "^2.0.2",
		"tslib": "^1.9.3",
		"url-polyfill": "^1.1.5",
		"web-animations-js": "^2.3.1",
		"whatwg-fetch": "^3.0.0",
		"xhr-polyfill": "^0.1.8",
		"zone.js": "^0.9.0",
		"proto-polyfill": "^1.7.0"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"browser": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typings": "./dist/index.d.ts",
	"es2015": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/wessberg/polyfiller.git"
	},
	"bugs": {
		"url": "https://github.com/wessberg/polyfiller/issues"
	},
	"contributors": [
		{
			"name": "Frederik Wessberg",
			"email": "frederikwessberg@hotmail.com",
			"url": "https://github.com/wessberg",
			"imageUrl": "https://avatars2.githubusercontent.com/u/20454213?s=460&v=4",
			"role": "Lead Developer",
			"twitter": "FredWessberg"
		}
	],
	"engines": {
		"node": ">=9.0.0"
	},
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
