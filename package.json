{
	"name": "@wessberg/polyfiller",
	"private": true,
	"version": "0.0.105",
	"description": "Never worry about polyfills again.",
	"files": [
		"dist/**/*.*",
		"polyfill-lib/**/*.*"
	],
	"scripts": {
		"serve": "node ./dist/index.js",
		"serve:production": "NODE_ENV=production CLEAR_CACHE=true npm run serve",
		"generate:scaffold": "scaffold all --yes",
		"generate:changelog": "standard-changelog --first-release",
		"generate:all": "npm run generate:scaffold && npm run generate:changelog",
		"clean": "rimraf dist",
		"lint": "tsc --noEmit && eslint \"src/**/*.ts\" --color",
		"prettier": "prettier --write '{src,test,documentation}/**/*.{js,ts,json,html,xml,css,md}'",
		"pretest": "rimraf compiled && npm run rollup:test",
		"test": "HOST=polyfiller.test NODE_ENV=TEST ava",
		"posttest": "rimraf compiled",
		"prebuild": "npm run clean",
		"build": "npm run rollup",
		"watch": "npm run rollup -- --watch",
		"rollup": "rollup -c rollup.config.js",
		"rollup:test": "rollup -c rollup.test.config.js",
		"scripts:markdown-format-feature-names": "ts-node --project scripts/tsconfig.json scripts/markdown-format-feature-names.ts",
		"preversion": "npm test && NODE_ENV=production npm run build",
		"version": "npm run generate:all && git commit -am \"docs: update CHANGELOG\" && git push",
		"release": "npm run version && node publish.js",
		"update": "npx npm-check-updates -u -x np --dep dev,prod && npm update && npm install"
	},
	"keywords": [
		"polyfills",
		"shim",
		"web",
		"webservice",
		"typescript",
		"polyfill-app",
		"polyfills-as-a-service"
	],
	"devDependencies": {
		"@types/astring": "1.3.0",
		"@types/mime": "2.0.3",
		"@types/node": "14.11.8",
		"@types/semver": "7.3.4",
		"@typescript-eslint/eslint-plugin": "4.4.1",
		"@typescript-eslint/parser": "4.4.1",
		"@wessberg/scaffold": "1.0.32",
		"eslint": "7.11.0",
		"eslint-config-prettier": "6.12.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsdoc": "30.6.4",
		"fs-extra": "^9.0.1",
		"husky": "4.3.0",
		"prettier": "2.1.2",
		"pretty-quick": "3.0.2",
		"rimraf": "3.0.2",
		"standard-changelog": "2.0.24",
		"ts-node": "9.0.0",
		"useragent-generator": "1.1.1-amkt-22079-finish.0"
	},
	"dependencies": {
		"@wessberg/di-compiler": "2.1.1",
		"@wessberg/rollup-plugin-ts": "1.3.5",
		"typescript": "4.0.3",
		"@wessberg/ts-config": "1.0.19",
		"@babel/core": "7.11.6",
		"@babel/plugin-transform-block-scoping": "7.11.1",
		"@webcomponents/custom-elements": "1.4.2",
		"@webcomponents/shadycss": "1.10.1",
		"@webcomponents/shadydom": "1.7.4",
		"@webcomponents/template": "1.4.3",
		"@wessberg/browserslist-generator": "1.0.39",
		"@wessberg/di": "2.0.3",
		"@wessberg/fileloader": "1.1.12",
		"@wessberg/filesaver": "1.0.11",
		"@wessberg/pointer-events": "1.0.9",
		"@wessberg/stringutil": "1.0.19",
		"Base64": "1.1.0",
		"astring": "1.4.3",
		"ava": "3.13.0",
		"blob-polyfill": "4.0.20200601",
		"chalk": "4.1.0",
		"console-polyfill": "0.3.0",
		"construct-style-sheets-polyfill": "2.4.2",
		"core-js": "3.6.5",
		"core-js-builder": "3.6.5",
		"devcert": "1.1.3",
		"events-polyfill": "2.1.2",
		"find-up": "5.0.0",
		"http-status-codes": "2.1.4",
		"intersection-observer": "0.11.0",
		"intl": "1.2.5",
		"intl-list-format": "1.0.3",
		"intl-relative-time-format": "1.0.7",
		"mime": "~2.4.6",
		"mutationobserver-shim": "0.3.7",
		"node.parentelement": "1.0.2",
		"nodemon": "2.0.5",
		"@polyfiller/object-fit": "^0.0.39",
		"perfnow": "0.2.0",
		"polyfill-library": "3.96.0",
		"proxy-polyfill": "0.3.2",
		"regenerator-runtime": "0.13.7",
		"requestanimationframe": "0.0.23",
		"requestidlecallback": "0.3.0",
		"rollup": "2.30.0",
		"@rollup/plugin-commonjs": "15.1.0",
		"@rollup/plugin-multi-entry": "4.0.0",
		"@rollup/plugin-node-resolve": "9.0.0",
		"scroll-behavior-polyfill": "2.0.13",
		"semver": "7.3.2",
		"setimmediate": "1.0.5",
		"systemjs": "6.7.1",
		"temp-dir": "2.0.0",
		"toposort": "2.0.2",
		"tslib": "2.0.3",
		"url-polyfill": "1.1.10",
		"web-animations-js": "2.3.2",
		"whatwg-fetch": "3.4.1",
		"xhr-polyfill": "0.1.8",
		"focus-visible": "5.2.0",
		"zone.js": "0.11.1"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"browser": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typings": "./dist/index.d.ts",
	"es2015": "./dist/index.js",
	"funding": {
		"type": "github",
		"url": "https://github.com/wessberg/polyfiller?sponsor=1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wessberg/polyfiller.git"
	},
	"bugs": {
		"url": "https://github.com/wessberg/polyfiller/issues"
	},
	"contributors": [
		{
			"name": "Frederik Wessberg",
			"email": "frederikwessberg@hotmail.com",
			"url": "https://github.com/wessberg",
			"imageUrl": "https://avatars2.githubusercontent.com/u/20454213?s=460&v=4",
			"github": "wessberg",
			"role": "Lead Developer",
			"twitter": "FredWessberg"
		}
	],
	"engines": {
		"node": ">=10.16.0"
	},
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"ava": {
		"files": [
			"compiled/*.test.*"
		],
		"verbose": true,
		"timeout": "40s",
		"extensions": [
			"js"
		],
		"environmentVariables": {
			"NODE_OPTIONS": "--max_old_space_size=4096"
		}
	}
}
